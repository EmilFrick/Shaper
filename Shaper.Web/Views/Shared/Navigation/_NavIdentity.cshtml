@inject IHttpContextAccessor contextAcc


@if (contextAcc.HttpContext.Session.GetString("JwToken") != null && contextAcc.HttpContext.Session.GetString("JwToken").ToString().Length > 0)
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="User" asp-controller="Account" asp-action="Logout">Logout</a>
    </li>
}
else
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="User" asp-controller="Account" asp-action="Register">Register</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="User" asp-controller="Account" asp-action="Login">Login</a>
    </li>
}